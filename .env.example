# =============================================================================
# Cloudflare R2 Storage Configuration
# =============================================================================
# How to get these credentials:
# 1. Go to Cloudflare Dashboard -> R2 Object Storage
# 2. Click "Manage R2 API Tokens" -> "Create API Token"
# 3. Select "Admin Read & Write" permission and create the token
# 4. Copy the generated Access Key ID and Secret Access Key
#
# Account ID can be found in: Cloudflare Dashboard -> any domain -> Overview
# (shown in the right sidebar under "API" section)
# =============================================================================

R2_ACCOUNT_ID=your_cloudflare_account_id
R2_ACCESS_KEY_ID=your_r2_access_key_id
R2_SECRET_ACCESS_KEY=your_r2_secret_access_key
R2_BUCKET_NAME=your_bucket_name

# =============================================================================
# R2 Public Access Domain
# =============================================================================
# Your R2 bucket's public access URL or custom domain
# Options:
#   - R2.dev subdomain: https://pub-xxxxxxxx.r2.dev
#   - Custom domain: https://r2.yourdomain.com
#
# To enable public access:
# 1. Go to R2 -> Your Bucket -> Settings -> Public Access
# 2. Enable "r2.dev subdomain" or connect a custom domain
# =============================================================================

R2_PUBLIC_DOMAIN=https://r2.yourdomain.com

# =============================================================================
# Security Configuration: Random Salt
# =============================================================================
# Used for generating secure, unpredictable filenames
# Generate a random string (the more random, the better)
# Tip: Use a password generator or mash your keyboard randomly
# =============================================================================

FILENAME_SALT=random-string-abLM1sT5uk3QRCxyZ2dEHv3JfgNo5pi2

# =============================================================================
# RAW_NODE_LINKS
# =============================================================================
RAW_NODE_LINKS = `
vless://xxx@1.1.1.1:443?...#node1
vless://xxx@2.2.2.2:443?...#node2
`